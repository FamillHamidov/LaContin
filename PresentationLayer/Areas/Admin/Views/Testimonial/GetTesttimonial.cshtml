@model EntityLayer.Entities.Testimonial
@{
    ViewData["Title"] = "GetTesttimonial";
    Layout = "~/Views/Shared/AdminLayout.cshtml";
}
<h1 style="margin-top:10px">Güncəlləmə səhifəsi</h1>
<form asp-action="UpdateGetInTouch" method="post">
	<input asp-for="Id" type="hidden" />
	<div>
		<label>Ad soyad</label>
		<input asp-for="Name" type="text" class="form-control" />
		<span asp-validation-for="Name" class="text-danger"></span>
	</div>
	<div>
		<label>Başlıq</label>
		<input asp-for="Title" type="text" class="form-control" />
		<span asp-validation-for="Title" class="text-danger"></span>
	</div>
	<div>
		<label>Açıqlama</label>
		<input asp-for="Description" type="text" class="form-control" />
		<span asp-validation-for="Description" class="text-danger"></span>
	</div>
	<div class="form-group" style="margin-top:10px">
		<button class="btn btn-primary">Güncəllə</button>
	</div>
</form>

