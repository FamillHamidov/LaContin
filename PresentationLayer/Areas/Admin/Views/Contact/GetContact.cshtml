@model EntityLayer.Entities.Contact
@{
    ViewData["Title"] = "GetContact";
    Layout = "~/Views/Shared/AdminLayout.cshtml";
}
<h1 style="margin-top:10px">Güncəlləmə Səhifəsi</h1>
<form method="post" asp-action="UpdateContact">
	<input type="hidden" asp-for="Id"/>
	<div class="form-group"  style="margin-top:10px">
		<label asp-for="Title"></label>
		<input asp-for="Title" type="text" class="form-control" />
	</div>
	<div class="form-group" style="margin-top:10px">
		<label asp-for="FirstSocialMedia"></label>
		<input asp-for="FirstSocialMedia" type="text" class="form-control" />
	</div>
	<div class="form-group" style="margin-top:10px">
		<label asp-for="FirstUrlOrNumber"></label>
		<input asp-for="FirstUrlOrNumber" type="text" class="form-control" />
	</div>
	<div class="form-group" style="margin-top:10px">
		<label asp-for="SecondSocialMedia"></label>
		<input asp-for="SecondSocialMedia" type="text" class="form-control" />
	</div>
	<div class="form-group" style="margin-top:10px">
		<label asp-for="SecondUrlOrNumber"></label>
		<input asp-for="SecondUrlOrNumber" type="text" class="form-control" />
	</div>
	<div class="form-group" style="margin-top:10px">
		<button class="btn btn-success">Güncəllə</button>
	</div>
</form>

