@model EntityLayer.Dtos.LogoDto
@{
    ViewData["Title"] = "GetLogo";
    Layout = "~/Views/Shared/AdminLayout.cshtml";
}

<h1>GetLogo</h1>
<form method="post" asp-action="UpdateLogo" enctype="multipart/form-data">
    <input asp-for="Id" type="hidden" />
    <input asp-for="PictureUrl" type="hidden" />
    <div class="form-group">
        <label asp-for="Picture"></label>
        <input asp-for="Picture" type="file" class="form-control" />
    </div>
    <div class="form-group">
        <button class="btn btn-primary">Update Logo</button>
    </div>
</form>