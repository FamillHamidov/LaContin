@model EntityLayer.Dtos.LogoDto
@{
    ViewData["Title"] = "AddLogo";
    Layout = "~/Views/Shared/AdminLayout.cshtml";
}
<h1>AddImage</h1>
<form method="post" enctype="multipart/form-data">
    <div class="form-group">
        <label asp-for="Picture"></label>
        <input asp-for="Picture" type="file" class="form-control" required />
    </div>
    <div class="form-group">
        <button class="btn btn-primary">Add New Logo</button>
    </div>
</form>

