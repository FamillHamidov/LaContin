@model EntityLayer.Entities.GetInTouch
@{
	ViewData["Title"] = "NewGetInTouch";
	Layout = "~/Views/Shared/AdminLayout.cshtml";
}

<h1 style="margin-top:10px">Yeni əlaqə vasitəsi</h1>
<form method="post">
	<div class="form-group" style="margin-top:10px">
		<label>Mail</label>
		<input asp-for="Email" type="text" class="form-control" />
	</div>
	<div class="form-group" style="margin-top:10px">
		<label>Nömrə</label>
		<input asp-for="Phone" type="text" class="form-control" />
	</div>
	<div class="form-group" style="margin-top:10px">
		<label>Adres</label>
		<input asp-for="Address" type="text" class="form-control" />
	</div>
	<div class="form-group" style="margin-top:10px">
		<button class="btn btn-success">Əlavə et</button>
	</div>
</form>
