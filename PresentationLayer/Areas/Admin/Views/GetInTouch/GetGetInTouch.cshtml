@model EntityLayer.Entities.GetInTouch
@{
	ViewData["Title"] = "GetGetInTouch";
	Layout = "~/Views/Shared/AdminLayout.cshtml";
}

<h1 style="margin-top:10px">Güncəlləmə səhifəsi</h1>
<form asp-action="UpdateGetInTouch" method="post">
	<input asp-for="Id" type="hidden" />
	<div>
		<label>Mail</label>
		<input asp-for="Email" type="text" class="form-control" />
	</div>
	<div>
		<label>Nömrə</label>
		<input asp-for="Phone" type="text" class="form-control" />
	</div>
	<div>
		<label>Adres</label>
		<input asp-for="Address" type="text" class="form-control" />
	</div>
	<div class="form-group" style="margin-top:10px">
		<button class="btn btn-primary">Güncəllə</button>
	</div>
</form>
