@using EntityLayer.Entities
@model List<Category>
@{
    Layout = null;
}

<li>
    <a href="#">Kateqoriyalar</a>
    <ul class="sub-menu-style">
        @foreach (var item in Model)
        {
            if (item.Status==true)
            {
                <li><a href="/Shop/Index/@item.Id">@item.Name </a></li>
            }
        }
    </ul>
</li>